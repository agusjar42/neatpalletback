{
  "name": "neatpallet_back",
  "version": "0.0.1",
  "description": "Proyecto API encargado de ser la parte Backend de la aplicaci√≥n web NEATPALLET",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "18 || 20 || 22"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run eslint && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "posttest": "npm run lint",
    "premigrate": "npm run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run rebuild",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Agusjar <agusjar@hotmail.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@anthropic-ai/sdk": "^0.65.0",
    "@loopback/authentication": "^11.0.2",
    "@loopback/authentication-jwt": "^0.15.2",
    "@loopback/authorization": "^0.15.2",
    "@loopback/boot": "^7.0.2",
    "@loopback/core": "^6.1.9",
    "@loopback/repository": "^7.0.2",
    "@loopback/rest": "^14.0.2",
    "@loopback/rest-explorer": "^7.0.2",
    "@loopback/security": "^0.11.12",
    "@loopback/service-proxy": "^7.0.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.5",
    "fs-extra": "^11.2.0",
    "isemail": "^3.2.0",
    "jsonwebtoken": "^9.0.2",
    "loopback-connector-mysql": "^7.0.10",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.16",
    "nvm": "^0.0.4",
    "qrcode": "^1.5.4",
    "sharp": "^0.33.5",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^11.0.2",
    "@loopback/eslint-config": "^15.0.2",
    "@loopback/testlab": "^7.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/multer": "^1.4.12",
    "@types/node": "^16.18.96",
    "@types/nodemailer": "^6.4.17",
    "@types/qrcode": "^1.5.5",
    "eslint": "^8.57.0",
    "source-map-support": "^0.5.21",
    "typescript": "~5.2.2"
  }
}
